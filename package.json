{
  "name": "dpk",
  "version": "0.1.0",
  "description": "Web interface for work MySQL DB",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV='production' webpack -p --progress",
    "build:watch": "webpack --watch",
    "nodemon": "NODE_PATH=./src nodemon server.babel.js --exec babel-node --presets es2015,stage-0,stage-3",
    "webpack-dev": "webpack-dev-server --watch --progress --debug --hot --devtool eval-source-map --output-pathinfo --colors --config webpack.config.js --inline --content-base public --port 8050 --host 127.0.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      "stage-3"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "keywords": [
    "react",
    "dpk",
    "isomorphic",
    "MySQl"
  ],
  "author": "Chugunov Roman <Zebs-BMK@yandex.ru> (https://github.com/ChugunovRoman)",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "express": "^4.15.4",
    "generic-pool": "^3.1.8",
    "mysql": "^2.14.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "sequelize": "^4.8.4",
    "universal-webpack": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "json-loader": "^0.5.7",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^1.3.1",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
